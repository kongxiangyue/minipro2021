/*页面样式*/
page {
  background-color: skyblue;
  color: black;
}
/*标题样式*/
.title {
  font-size: 40rpx;
  height: 72rpx;
  line-height: 72rpx;
  padding-left: 20rpx;
}
/*分割线样式*/
.hr {
  margin: 12px;
  width: 100%;
  height: 1px;
  background-color: grey;
}
/*文字样式*/
text {
  padding-left: 20rpx;
  font-size: 30rpx;
}
/*选项样式*/
.item {
  margin: 12rpx;
}
/*标签样式*/
.item label {
  margin: 10px;
}
